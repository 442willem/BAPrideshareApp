<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:textStyle="bold"
        android:id="@+id/rideTextView_begin"
        android:text="eindtijd"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:textStyle="bold"
        android:id="@+id/rideTextView_end"
        android:text="eindtijd"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:id="@+id/routeTextView_begin"
        android:textStyle="bold"
        android:text="begin"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:textStyle="bold"
        android:id="@+id/routeTextView_eind"
        android:text="eind"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:textStyle="bold"
        android:id="@+id/routeTextView_begintijd"
        android:text="begintijd"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:textStyle="bold"
        android:id="@+id/routeTextView_eindtijd"
        android:text="eindtijd"/>



</LinearLayout>